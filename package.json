{
  "name": "astra-mongoose",
  "version": "0.0.1",
  "description": "DataStax Astra's NodeJS mongoose compatability client",
  "author": "CRW <chris.wilhite@datastax.com>",
  "homepage": "https://github.com/datastax-labs/astra-mongoose",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "mocha": {
    "timeout": 30000,
    "require": [
      "tests/setup.ts"
    ]
  },
  "directories": {
    "lib": "src",
    "test": "tests"
  },
  "files": [
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datastax/astrajs.git"
  },
  "scripts": {
    "test": "nyc ts-mocha --paths -p tsconfig.json tests/**/*.test.ts",
    "build": "tsc --project tsconfig.build.json",
    "build:docs": "jsdoc2md -t README.hbs --files src/**/*.ts --configure ./jsdoc2md.json > README.md"
  },
  "bugs": {
    "url": "https://github.com/datastax-labs/astra-mongoose/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@ngneat/falso": "^5.4.0",
    "@types/expect": "^24.3.0",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.36",
    "dotenv": "^16.0.1",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^7.1.1",
    "nyc": "^15.1.0",
    "ts-mocha": "^10.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "lodash": "^4.17.21",
    "mongodb": "^4.6.0",
    "mongoose": "^6.3.4",
    "winston": "^3.7.2"
  }
}
